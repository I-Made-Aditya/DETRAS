	"QCustomSlideMenu": [{
		"name": "LeftMenuContainer",
		"defaultSize": [{
			"width": 45,
			"height": "parent"
		}],
		"collapsedSize": [{
			"width": 45,
			"height": "parent"
		}],
		"expandedSize": [{
			"width": 140,
			"height": "parent"
		}],
		"toggleButton": [{
			"buttonName": "menuBtn",
			"icons": [{
				"whenMenuIsCollapsed": ":/icons/icons/align-justify.svg",
				"whenMenuIsExpanded": ":/icons/icons/chevron-left.svg"
			}]
		}],
		"menuTransitionAnimation": [{
			"animationDuration": 400,
			"animationEasingCurve": "Linear"
		}]
	},
	{
		"name": "popupNotificationContainer",
		"defaultSize": [{
			"width": 0,
			"height": "auto"
		}],
		"collapsedSize": [{
			"width": 0,
			"height": "auto"
		}],
		"expandedSize": [{
			"width": 500,
			"height": 100
		}],
		"toggleButton": [{
			"buttonName": "notificationBtn",
			"icons": [{
				"whenMenuIsCollapsed": ":/icons/icons/bell.svg",
				"whenMenuIsExpanded": ":/icons/icons/bell-off.svg"
			}]
		}],
		"menuTransitionAnimation": [{
			"animationDuration": 200,
			"animationEasingCurve": "Linear"
		}],
